{
  "name": "lecard-whatsapp",
  "author": "storkdigital.com.br",
  "description": "WhatsApp Bot",
  "version": "1.3.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "electron-trace": "electron --trace-warnings .",
    "realese": "electron-builder -wl -- -p always"
  },
  "build": {
    "asar": "true",
    "productName": "LeBot",
    "appId": "delivery.lecard.whatsapp",
    "artifactName": "lebot-${version}.${ext}",
    "mac": {
      "icon": "./icons/icon.icns",
      "category": "public.app-category.utilities"
    }
  },
  "devDependencies": {
    "electron": "^17.1.0"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "electron-config": "^2.0.0",
    "electron-fetch": "^1.7.4",
    "electron-updater": "^4.6.5",
    "form-data": "^4.0.0",
    "socket.io-client": "^2.4.0"
  }
}
