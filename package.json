{
  "name": "lecard-whatsapp",
  "author": "storkdigital.com.br",
  "description": "WhatsApp Bot",
  "version": "1.3.5",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "electron-trace": "electron --trace-warnings .",
    "build": "electron-builder --x64 --ia32",
    "realese": "electron-builder --x64 --ia32 --publish always"
  },
  "build": {
    "asar": "true",
    "productName": "LeBot - LeCard ChatBot",
    "appId": "delivery.lecard.whatsapp",
    "artifactName": "lebot-${version}.${ext}"
  },
  "devDependencies": {
    "electron": "^17.1.0",
    "electron-builder": "~22.10.5"
  },
  "dependencies": {
    "electron-config": "^2.0.0",
    "electron-fetch": "^1.7.4",
    "electron-updater": "^4.6.5",
    "form-data": "^4.0.0",
    "socket.io-client": "^2.4.0"
  }
}
